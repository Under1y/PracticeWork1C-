#include <iostream> 
#include <locale> 
 
int main() { 
    setlocale(LC_ALL, "Rus"); 
     
    std::cout << "Во глубине сибирских руд\n"; 
    std::cout << "Храните гордое терпенье,\n"; 
    std::cout << "Не пропадет ваш скорбный труд\n"; 
    std::cout << "И дум высокое стремленье.\n"; 
     
    return 0; 
}